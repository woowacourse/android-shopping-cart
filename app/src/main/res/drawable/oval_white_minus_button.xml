<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="48dp"
                android:height="48dp"/>
            <padding
                android:left="12dp"
                android:right="12dp"
                android:top="12dp"
                android:bottom="12dp"/>
            <solid
                android:color="@color/white"/>
        </shape>
    </item>

    <item>
        <shape android:shape="line">
            <size
                android:width="18dp"/>
            <stroke
                android:width="3dp"
                android:color="@color/gray_700"/>
        </shape>
    </item>
</layer-list>
